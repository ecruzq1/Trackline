            <a href="/logout" class="list-group-item list-group-item-action waves-effect">
                <i class="fas fa-sign-out-alt mr-3"></i>Cerrar Sesión</a>
        </div>
    </div>
    <div class="content-panel white rounded-lg pt-5 mb-5 border">
        <div class="header pt-5">
            <h2 class="font-weight-normal font-italic text-center">Componentes del sistema</h2>
        </div>
        <table class="table table-bordered mt-5">
            <thead>
                <tr>
                    <th>
                        <center>Componente</center>
                    </th>
                    <th>
                        <center>Acción</center>
                    </th>
            </thead>
            <tbody>
                <tr>
                    <td>Tipos de Inmuebles</td>
                    <td>
                        <center><a href="/tiposInmuebles" type="button" class="btn btn-light-green btn-sm m-0 rnd">Gestionar</a>
                        </center>
                    </td>
                </tr>
                <tr>
                    <td>Grupo de Características de Inmuebles</td>
                    <td>
                        <center><a href="/grupoCaracteristicas" type="button"
